{
  "kind": "StatefulSet",
  "id": "ci",
  "image": "liferaycloud/jenkins:2.249.3-4.1.0",
  "deploy": false,
  "memory": 8192,
  "cpu": 4,
  "scale": 1,
  "volumes": {
    "ciData": "/var/jenkins_home"
  },
  "loadBalancer": {
    "targetPort": 8080
  },
  "readinessProbe": {
    "httpGet": {
      "path": "/login",
      "port": 8080
    },
    "initialDelaySeconds": 60,
    "timeoutSeconds": 10,
    "failureThreshold": 12
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/login",
      "port": 8080
    },
    "initialDelaySeconds": 300,
    "timeoutSeconds": 10,
    "failureThreshold": 12
  },
  "env": {
    "JENKINS_ADMIN_USER_NAME": "argusgroup",
    "JENKINS_EMAIL_NOTIFICATIONS_FROM": "Jenkins CI - argusgroup <argusgroup@liferay.com>",
    "JENKINS_URL": "https://ci-argusgroup-infra.lfr.cloud",
    "LCP_CI_LIFERAY_DXP_HOTFIXES_COMMON": "liferay-hotfix-4006-7210",
    "LCP_CI_SCM_PROVIDER": "github",
    "LCP_CI_SCM_REPOSITORY_NAME": "Liferay",
    "LCP_CI_SCM_REPOSITORY_OWNER": "argus-group",
    "LCP_CI_USE_DEFAULT_JENKINSFILE": "true",
    "NODE_ENV": "production"
  },
  "environments": {
    "infra": {
      "deploy": true
    }
  }
}