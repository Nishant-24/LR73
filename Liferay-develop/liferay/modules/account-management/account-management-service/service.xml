<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="com.argus.oth.account.management">
	<namespace>OTH_Account</namespace>
	<entity local-service="true" name="AMAnalyticsClaim" remote-service="false" table="OTH_AMAnalyticsClaim">

		<!-- PK fields -->

		<column name="amAnalyticsClaimId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />
		<column name="companyId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="clientId" type="long" />
		<column name="amAnalyticsClaimDate" type="Date" />
		<column name="avgClaimTurnAround" type="int" />
		<column name="minClaimTurnAround" type="int" />
		<column name="maxClaimTurnAround" type="int" />
		<column name="claimTurnAroundGoal" type="int" />
		<column name="overallAvgClaimTurnAround" type="double" />

		<order by="asc">
			<order-column name="amAnalyticsClaimDate" />
		</order>
	</entity>
	<entity name="StagingAMAnalyticsClaim" local-service="true" remote-service="false" table="OTH_StagingAMAnalyticsClaim">
		<!-- PK Fields -->

		<column name="stagingAMAnalyticsClaimId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="clientId" type="long" />
		<column name="amAnalyticsClaimDate" type="String" />
		<column name="avgClaimTurnAround" type="int" />
		<column name="minClaimTurnAround" type="int" />
		<column name="maxClaimTurnAround" type="int" />
		<column name="claimTurnAroundGoal" type="int" />
		<column name="overallAvgClaimTurnAround" type="double" />

		<order by="asc">
			<order-column name="amAnalyticsClaimDate" />
		</order>
	</entity>
	<entity local-service="true" name="AMAnalyticsDB" remote-service="false" table="OTH_AMAnalyticsDB">
		<!-- PK fields -->

		<column name="amAnalyticsDbId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />
		<column name="companyId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="clientId" type="long" />
		<column name="savingsPercent" type="double" />
		<column name="chargedAmount" type="double" />
		<column name="repricedAmount" type="double" />
		<column name="avgDownTime" type="double" />
		<column name="avgSpeedOfAnswer" type="double" />
		<column name="abandonmentRate" type="double" />
		<column name="claimsTurnaround" type="long" />
		<column name="downTimeGoal" type="double" />
		<column name="speedOfAnswerGoal" type="double" />
		<column name="abandonmentRateGoal" type="double" />
		<column name="claimsTurnaroundGoal" type="double" />

		<order by="asc">
			<order-column name="amAnalyticsDbId" />
		</order>

		<!-- Finder -->

		<finder name="ClientId" return-type="AMAnalyticsDB">
			<finder-column name="clientId" />
		</finder>
	</entity>
	<entity name="StagingAMAnalyticsDB" local-service="true" remote-service="false" table="OTH_StagingAMAnalyticsDB">
		<!-- PK fields -->

		<column name="stagingAMAnalyticsDbId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="clientId" type="long" />
		<column name="savingsPercent" type="double" />
		<column name="chargedAmount" type="double" />
		<column name="repricedAmount" type="double" />
		<column name="avgDownTime" type="double" />
		<column name="avgSpeedOfAnswer" type="double" />
		<column name="abandonmentRate" type="double" />
		<column name="claimsTurnaround" type="long" />
		<column name="downTimeGoal" type="double" />
		<column name="speedOfAnswerGoal" type="double" />
		<column name="abandonmentRateGoal" type="double" />
		<column name="claimsTurnaroundGoal" type="double" />

		<order by="asc">
			<order-column name="stagingAMAnalyticsDbId" />
		</order>
	</entity>
	<entity local-service="true" name="AMAnalyticsTelephony" remote-service="false" table="OTH_AMAnalyticsTelephony">
