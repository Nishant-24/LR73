		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="clientId" type="long" />
		<column name="amAnalyticsTelephonyDate" type="String" />
		<column name="avgDownTime" type="double" />
		<column name="avgSpeedOfAnswer" type="double" />
		<column name="abandonmentRate" type="double" />
		<column name="downTimeGoal" type="double" />
		<column name="speedAnswerGoal" type="double" />
		<column name="abandonmentRateGoal" type="double" />
		<column name="overallAvgDownTime" type="double" />
		<column name="overallAvgSpeedOfAnswer" type="double" />
		<column name="overallAvgAbandonmentRate" type="double" />

		<order by="asc">
